<script type="text/javascript">
    // <![CDATA[

    function initImages() {
        var re = /_t\.jpg$/;
        $$('div.img a').each(function(el){
            var img = el.down('img');
            if (img) {
                if (re.test(img.src)) {
                    el.href = img.src.replace(re,'.jpg');
                    el.observe('click', function(e){ window.open(this.href); e.stop(); });
                }
            }
        })
    }

    document.observe('dom:loaded', initImages);

    // ]]>
</script>

